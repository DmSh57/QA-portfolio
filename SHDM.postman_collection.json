		{
			"name": "List Users + часть массива",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Total pages count\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"total_pages\");\r",
							"});\r",
							"pm.test(\"Total users count\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.total).to.eql(12);\r",
							"});\r",
							"pm.test(\"Broken test from video\", function () {\r",
							"    var jsonData = pm.response.json(\"data\");\r",
							"    pm.expect(jsonData.data[1].first_name).to.eql(\"Lindsay\");\r",
							"});\r",
							""
						],
